{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    },
    {
      "src": "/pdf.worker.js",
      "dest": "/pdf.worker.js"
    }
  ],
  "functions": {
    "api/*.ts": {
      "memory": 3008,
      "maxDuration": 120
    }
  }
}





